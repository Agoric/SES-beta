{
  "name": "evaluator-shim",
  "version": "2.0.0",
  "description": "Spec-compliant shim for Compartment TC39 Proposal",
  "type": "module",
  "main": "dist/evaluator-shim.cjs.js",
  "module": "dist/evaluator-shim.esm.js",
  "browser": "dist/evaluator-shim.umd.js",
  "license": "Apache-2.0",
  "author": {
    "name": "TC39 Open Source Contributors"
  },
  "keywords": [
    "realms",
    "ES8",
    "ES2017",
    "ECMAScript",
    "ESNext",
    "proposal"
  ],
  "homepage": "https://github.com/Agoric/evaluator-shim#readme",
  "bugs": {
    "url": "https://github.com/Agoric/evaluator-shim/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agoric/evaluator-shim.git"
  },
  "scripts": {
    "lint": "eslint *.js ./src ./test ./demo",
    "prettier": "prettier --config ./.prettierrc --write '*.js' './src/**/*.js' './test/**/*.js' './demo/**/*.js'",
    "test": "tape -r esm 'test/**/*.js' | tap-spec",
    "debug": "node --inspect-brk -r esm",
    "coverage": "nyc tape -r esm 'test/**/*.test.js'",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "rollup -c rollup.config.js",
    "watch": "build --watch"
  },
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "esm": "^3.2.25",
    "mixed-tape": "^1.0.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rollup": "^1.24.0",
    "rollup-plugin-terser": "^5.1.2",
    "sinon": "^7.5.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0"
  },
  "files": [
    "dist",
    "LICENSE*"
  ]
}
