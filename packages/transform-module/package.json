{
  "name": "@agoric/transform-module",
  "version": "0.3.0",
  "description": "Transform for evaluating ES modules as Javascript programs",
  "main": "dist/transform-module.cjs.js",
  "module": "dist/transform-module.esm.js",
  "browser": "dist/transform-module.umd.js",
  "scripts": {
    "build": "rollup -c",
    "test": "tape -r esm 'test/**/*.js'",
    "lint-fix": "eslint --fix '**/*.js'",
    "lint": "eslint '**/*.js'"
  },
  "devEngines": {
    "node": "8.x || 9.x || 10.x || 11.x || 12.x"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "^7.12.0",
    "prettier": "1.19.1",
    "tap-spec": "^5.0.0",
    "tape": "4.12.1",
    "tape-promise": "^4.0.0"
  },
  "dependencies": {
    "@agoric/evaluate": "^2.1.3",
    "@babel/core": "7.7.5",
    "esm": "3.2.25",
    "rollup": "1.27.9",
    "rollup-plugin-node-resolve": "5.2.0"
  },
  "keywords": [
    "ESM",
    "ECMAScript Modules",
    "import",
    "export"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agoric/SES.git"
