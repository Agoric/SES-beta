{
  "name": "xscm",
  "include": [
    "$(MODDABLE)/examples/manifest_base.json"
  ],
  "build": {
    "BUILD": "$(MODDABLE)/build",
    "SES": "$(HOME)/ses",
    "MODULES": "$(MODDABLE)/modules"
  },
  "creation": {
    "keys": {
      "available": 65536
    },
    "stack": 4194304,
    "parser": {
      "buffer": 4194304
    }
  },
  "strip": [],
  "modules": {
    "main": "./src/bootstrap-stage-1",
    "pwd": "./pwd",
    "text": "$(MODDABLE)/modules/data/text/text",
    "file": "$(MODULES)/files/file/*",
    "@moddable/Resource": [
      "$(MODULES)/files/resource/Resource"
    ],
    "src/date-shim": "./src/date-shim",
    "src/console-shim": "./src/console-shim",
    "src/console": "./src/console",
    "src/xscm": "./src/xscm"
  },
  "data": {
    "*": [
      "dist/*"
    ]
  },
  "platforms": {
    "mac": {
      "modules": {
        "file": "$(MODULES)/files/file/mac/*"
      }
    },
    "x-cli-lin": {
      "modules": {
        "*": [
          "$(MODULES)/base/instrumentation/*",
          "$(MODULES)/files/file/lin/*"
        ]
      }
    }
  }
}
