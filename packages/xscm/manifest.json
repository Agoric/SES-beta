{
  "name": "xscm",
  "include": [
    "$(MODDABLE)/examples/manifest_base.json",
    "$(MODDABLE)/modules/files/file/manifest.json"
  ],
  "build": {
    "BUILD": "$(MODDABLE)/build",
    "MODULES": "$(MODDABLE)/modules"
  },
  "creation": {
    "keys": {
      "available": 4096
    },
    "stack": 4096,
    "parser": {
      "buffer": 32768
    }
  },
  "strip": [],
  "modules": {
    "main": "./main",
    "text": "$(MODDABLE)/modules/data/text/text",
    "@agoric/.retain": "/dev/null",
    "@agoric/compartment-mapper/*": "/Users/kris/ses/packages/compartment-mapper/src/*",
    "@agoric/compartment-mapper/zip/*": "/Users/kris/ses/packages/compartment-mapper/src/zip/*",
    "@moddable/Resource": [
      "$(MODULES)/files/resource/Resource"
    ],
    "@moddable/timer": [
      "$(MODULES)/base/timer/timer",
      "$(MODULES)/base/timer/$(PLATFORM)/*"
    ],
    "src/console": "./src/console",
    "src/harden": "./src/harden",
    "src/endo-load": "./src/endo-load",
    "src/text-shim": "./src/text-shim",
    "src/xscm": "./src/xscm"
  },
  "platforms": {
    "x-mac-lin": {
      "modules": {
        "*": [
          "$(MODULES)/base/instrumentation/*",
          "$(MODULES)/files/file/*",
          "$(MODULES)/files/file/$(PLATFORM)/*"
        ]
      }
    }
  }
}
