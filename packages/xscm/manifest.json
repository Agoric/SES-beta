{
  "name": "xscm",
  "include": [
    "$(MODDABLE)/examples/manifest_base.json"
  ],
  "build": {
    "BUILD": "$(MODDABLE)/build",
    "SES": "$(HOME)/ses",
    "MODULES": "$(MODDABLE)/modules"
  },
  "creation": {
    "keys": {
      "available": 4096
    },
    "stack": 4096,
    "parser": {
      "buffer": 32768
    }
  },
  "strip": [],
  "modules": {
    "main": "./main",
    "pwd": "./pwd",
    "text": "$(MODDABLE)/modules/data/text/text",
    "@agoric/.retain": "/dev/null",
    "@agoric/compartment-mapper/*": "$(SES)/packages/compartment-mapper/src/*",
    "@agoric/compartment-mapper/zip/*": "$(SES)/packages/compartment-mapper/src/zip/*",
    "file": "$(MODULES)/files/file/*",
    "@moddable/Resource": [
      "$(MODULES)/files/resource/Resource"
    ],
    "src/console": "./src/console",
    "src/harden": "./src/harden",
    "src/endo-load": "./src/endo-load",
    "src/text-shim": "./src/text-shim",
    "src/xscm": "./src/xscm"
  },
  "platforms": {
    "mac": {
      "modules": {
	"file": "$(MODULES)/files/file/mac/*"
      }
    },
    "x-cli-lin": {
      "modules": {
        "*": [
          "$(MODULES)/base/instrumentation/*",
          "$(MODULES)/files/file/lin/*"
        ]
      }
    }
  }
}
